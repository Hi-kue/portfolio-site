{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.astro"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "astro/*": [
        "./src/astro/*"
      ],
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@aceternityui/*": [
        "src/components/react/*"
      ]
    },
    "jsxImportSource": "react"
  }
}